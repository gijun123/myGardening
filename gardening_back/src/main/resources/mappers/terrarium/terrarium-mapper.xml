<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ggirick.gardening_back.mappers.terrarium.ImageMapper">

    <select id="getAllAssets" resultType="com.ggirick.gardening_back.dto.terrarium.TerrariumAssetImageDTO">
        SELECT ID, CATEGORY, NAME, URL, CREATED_AT
        FROM TERRARIUM_ASSET_IMAGE
        ORDER BY ID ASC
    </select>
</mapper>